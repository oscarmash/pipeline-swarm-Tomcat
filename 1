pipeline {
    agent any

    stages {
        stage('Prepare') {
            steps {
		echo 'Verificar git..'
		checkout scm
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
		echo "BUILD NUMBER: ${env.BUILD_NUMBER}"
		echo "hostname"
		echo "pwd"
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
